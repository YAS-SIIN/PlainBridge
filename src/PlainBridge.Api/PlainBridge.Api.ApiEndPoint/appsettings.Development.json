{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.AspNetCore.Authentication": "Debug",
                "System": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "ApplicationSettings": {
        "PlainBridgeIdsUrl": "https://localhost:5003",
        "PlainBridgeWebUrl": "http://localhost:5004/",
        "PlainBridgeIdsClientId": "back",
        "PlainBridgeIdsClientSecret": "secret",
        "PlainBridgeIdsScope": "PlainBridge IdentityServerApi",
        "PlainBridgeUseHttp": true,
        "PlainBridgeWebRedirectPage": "sign-in-oidc",
        "HybridDistributedCacheExpirationTime": "48:00:00",
        "HybridMemoryCacheExpirationTime": "48:00:00",
        "HybridCacheMaximumPayloadBytes": 10485760,
        "HybridCacheMaximumKeyLength": 512
    }
}
