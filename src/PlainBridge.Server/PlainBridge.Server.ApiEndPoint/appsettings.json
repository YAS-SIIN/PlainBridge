{

    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.AspNetCore.Authentication": "Debug",
                "System": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "ApplicationSettings": {
        "DefaultDomain": ".server.plainbridge.com",
        "PlainBridgeApiAddress": "http://api.plainbridge.com",
        "PlainBridgeIdsUrl": "http://id.plainbridge.com",
        "PlainBridgeIdsClientId": "back",
        "PlainBridgeIdsClientSecret": "secret",
        "PlainBridgeIdsScope": "PlainBridge",
        "PlainBridgeUseHttp": true,
        "HybridDistributedCacheExpirationTime": "48:00:00",
        "HybridMemoryCacheExpirationTime": "48:00:00",
        "HybridCacheMaximumPayloadBytes": 10485760,
        "HybridCacheMaximumKeyLength": 512
    }
}
